<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN" packages="com.ageofmine">
    <Appenders>
        <Console name="SysOut" target="SYSTEM_OUT">
            <PatternLayout pattern="[%d{HH:mm:ss} %level %thread] %msg%n"/>
        </Console>
    </Appenders>
    <Loggers>
        <Root level="info">
            <AppenderRef ref="SysOut"/>
        </Root>
    </Loggers>
</Configuration>